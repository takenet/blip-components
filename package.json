{
  "name": "blip-components",
  "version": "7.12.0",
  "description": "Shared components used in portal.blip.ai",
  "main": "./dist/components.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --port 3000",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch --progress",
    "commit": "./node_modules/.bin/git-cz",
    "semantic-release": "semantic-release"
  },
  "author": "Samuel Martins",
  "license": "ISC",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/takenet/blip-components.git"
  },
  "homepage": "https://github.com/takenet/blip-components#readme",
  "devDependencies": {
    "@types/angular": "1.6.45",
    "@types/angular-translate": "^2.16.0",
    "@types/angular-ui-router": "^1.1.40",
    "@types/node": "14.14.20",
    "@types/webpack": "^4.4.24",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "commitizen": "^2.10.1",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^2.1.0",
    "cz-customizable": "^5.3.0",
    "cz-customizable-ghooks": "^1.5.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "ghooks": "^2.0.4",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "loader-utils": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "raw-loader": "^3.0.0",
    "resolve-url-loader": "2.3.0",
    "sass": "^1.60.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "ts-loader": "5.3.3",
    "tslint": "5.12.0",
    "typescript": "3.2.2",
    "url-loader": "1.0.1",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "angular-sanitize": "^1.8.2",
    "angular-tooltips": "^1.2.2",
    "angular-translate": "^2.15.2",
    "angular-translate-loader-partial": "^2.18.1",
    "angular-translate-storage-cookie": "^2.15.1",
    "babel-polyfill": "^6.16.0",
    "blip-ds": "^1.247.0",
    "blip-toolkit": "5.22.0",
    "fecha": "^2.3.3",
    "he": "^1.2.0",
    "moment": "^2.22.1",
    "ng-cache-loader": "0.0.15",
    "ng-toast": "^2.0.0",
    "ob-daterangepicker": "^0.12.1",
    "sanitize-html": "^1.27.4",
    "semantic-release": "^15.9.15",
    "tslib": "1.6.0",
    "uuid": "^8.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    },
    "ghooks": {
      "commit-msg": "cz-customizable-ghooks $2"
    }
  },
  "release": {
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "build",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "minor"
        }
      ]
    }
  }
}
